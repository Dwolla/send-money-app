{
  "name": "dwolla-sample-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "_eslint": "eslint . --ext .js --ext .jsx ",
    "_prettier": "prettier *.{js,json} \"./!(*node_modules|.next|coverage|out)/*.{js,jsx,css,scss}\" \"./!(*node_modules|.next|coverage|out)/**/*.{js,jsx,css,scss}\" \"./!(*node_modules|.next|coverage|out)/**/**/*.{js,jsx,css,scss}\"",
    "checks": "yarn _prettier --check && yarn _eslint",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prepare": "husky install"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^1.2.0",
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "dwolla-v2": "^3.2.0",
    "moment": "^2.29.1",
    "next": "10.0.9",
    "react": "17.0.1",
    "react-bootstrap": "^1.5.2",
    "react-dom": "17.0.1",
    "react-modal": "^3.13.1",
    "react-moment": "^1.1.1",
    "swr": "^0.5.4"
  },
  "devDependencies": {
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "prettier": "^2.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,css,scss,json}": "prettier --write",
    "*.{js,jsx}": "eslint --fix"
  }
}
